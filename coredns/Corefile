home {
    errors
    log {
      class all
    }
    rewrite stop type AAAA A
    hosts {
        # Static entries
        192.168.50.207 nuc1.home
        192.168.50.49 pi4.home
        fallthrough
    }
    docker {
        domain home
        ttl 60
    }
    cache 30
}

. {
    errors
    log
    health
    ready
    forward . 8.8.8.8 8.8.4.4
    cache 30
}
